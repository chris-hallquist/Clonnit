<h1>Create Subclon</h1>
<% if flash[:errors] && !flash[:errors].empty? %>
  <% flash[:errors].each do |error| %>
         <%= error %> <br />
        <% end %>
<% end %>
<form action="/subclons" method="post">
  <input
    name="authenticity_token"
    type="hidden"
    value="<%= form_authenticity_token %>">

  <label>Name</label>
  <input
    type="text"
    value=""
    name="subclon[name]">

  <input type="submit" value="Create">
</form>