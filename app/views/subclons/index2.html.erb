<div id="content">
</div>

<script type="application/json" id="bootstrapped_subclons_json">
	<%= Subclon.all.to_json.html_safe %>
</script>

<script type="application/javascript">
	$(function () {
		var subclons = JSON.parse($("#bootstrapped_subclons_json").html());
		
		window.Clonnit.initialize($("#content"), subclons);
	});
</script>
